{
    "name": "agung-maxsol/analytics",
    "description": "A package to retrieve Google Analytics data.",
    "keywords": [
        "google",
        "analytics",
        "retrieve",
        "reports"
    ],
    "homepage": "https://github.com/agung-maxsol/analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Agung Mahardika",
            "email": "agung.mahardika@maxsol.id",
            "homepage": "https://maxsol.id",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.1|^7.0"
    },
    "autoload": {
        "psr-4": {
          "AgungMaxsol\\Analytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AgungMaxsol\\Analytics\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
